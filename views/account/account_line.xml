                <tree string="Account Line Tree">
                    
                    <field name="fecha"/>
                    <field name="fechavencimiento"/>

                    <field name="tipodocumento"/>
                    <field name="numeroserie"/>
                    <field name="numerofactura"/>

                    <field name="tipodoc"/>
                    <field name="numdoc"/>
                    <field name="nombre"/>

                    <field name="EXPortacion"/>

                    <field name="neto"/>
                    <field name="exonerado"/>
                    <field name="inafecto"/>
                    <field name="isc"/>
                    <field name="igv"/>
                    <field name="otros"/>
                    <field name="total"/>


                    
                    <field name="product"/>
                    <field name="product_type"/>



                    <field name="tipocambio"/>

                    <field name="fechar"/>
                    <field name="tipor"/>
                    <field name="serier"/>
                    <field name="numr"/>

                    <field name="moneda"/>
                    <field name="dolares"/>
                    <field name="fechavencimiento2"/>
                    <field name="condicion"/>
                    <field name="ccosto"/>
                    <field name="ccosto2"/>

                    <field name="cuentab"/>
                    <field name="cuentao"/>
                    <field name="cuentacontable"/>

                    <field name="regimen"/>
                    <field name="porcen"/>
                    <field name="importer"/>

                    <field name="seried"/>
                    <field name="numdocr"/>
                    <field name="fechad"/>
                    <field name="codigop"/>

                    <field name="porigv"/>
                    <field name="glosa"/>

                    <field name="mediop"/>
                    <field name="comd"/>
                    <field name="importec"/>


<!--
                    <field name="cuo_mes"/>
                    <field name="cuo_sd"/>
                    <field name="cuo_asi"/>
                    <field name="account_id"/>
                    <field name="serial_nr"/>
                    <field name="x_type"/>
                    <field name="date_time"/>
                    <field name="document"/>
                    <field name="patient"/>
                    
                    <field name="amount_net"/>
                    <field name="amount_tax"/>
                    <field name="amount"/>
-->

                </tree>
            </field>
        </record>





<!-- Tree 2 --> 
        <record model="ir.ui.view" id="account_line_rv_view_tree">
            <field name="name">openhealth.account.line.tree</field>
            <field name="model">openhealth.account.line</field>
            <field name="arch" type="xml">
                <tree string="Account Line Tree">

                    <field name="serial_nr"/>
                    <field name="x_type"/>

                    <field name="date_time"/>
                    <field name="date_char"/>
                    <field name="time_char"/>



                    <field name="patient"/>
                    <field name="document"/>
                    <field name="document_type"/>



                    <field name="qty"/>
                    <field name="product"/>
                    <field name="product_type"/>
                    
                    <field name="state"/>

                    <field name="amount_net"/>
                    <field name="amount_tax"/>
                    <field name="amount"/>
                </tree>
            </field>
        </record>
<!--
-->






        <!-- Window Form Action -->
        <record model="ir.actions.act_window" id="account_line_form_action">
            <field name="name">Plantilla de Contasis</field>
            <field name="res_model">openhealth.account.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[
                                    ('account_id', '=', active_id),
                                ]
            </field>
            <field name="limit">1000</field>
        </record>





        <!-- Action 2 -->
        <record model="ir.actions.act_window" id="account_line_list_action">
            <field name="name">Registro de Ventas</field>
            <field name="res_model">openhealth.account.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[
                                    ('account_id', '=', active_id),
                                ]
            </field>

            <field name="view_id" ref="account_line_rv_view_tree"/> 

            <field name="limit">1000</field>

            <!--
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="view_id" eval="False"/>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Create the first order
                </p>
            </field>
            -->
        </record>








        <!-- FILTER --> 
        <record id="acount_line_filter" model="ir.ui.view">
            <field name="name">Account Line Search</field>
            <field name="model">openhealth.account.line</field>
            <field name="arch" type="xml">
                <search string="Account Lines">

                    <field  name="numerofactura"    />
                    
                    <group expand="1" string="Group By">

                        <filter name="tipodoc" context="{'group_by':'tipodoc'}"/>
                        <separator/>                        
                        <filter name="tipodocumento" context="{'group_by':'tipodocumento'}"/>

                        <filter name="Producto" context="{'group_by':'product'}"/>
                        <filter name="Tipo de producto" context="{'group_by':'product_type'}"/>

                    </group>
               </search>
           </field>
       </record>



    </data>
</openerp>
