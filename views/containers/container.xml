<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>



<!-- FORM --> 
		<record model="ir.ui.view" id="container_form_view">
			<field name="name">container.form</field>
			<field name="model">openhealth.container</field>
			<field name="priority" eval="1"/>
			<field name="arch" type="xml">
				<form string="container Form">
					<header>

						<!-- Create Electronic --> 
						<button 
							type="object" 
							class="oe_highlight"
							name="create_electronic" 
							
							string="Create Electronic" 								
						/>

						<!-- Export Txt --> 
						<button 
							type="object" 
							class="oe_highlight"
							name="export_txt" 
							
							string="Export Txt" 								
						/>

						<!-- QC --> 
						<button 
							type="object" 
							class="oe_highlight"
							name="test_qc" 
							
							string="QC" 								
						/>




						<!-- Correct --> 

<!--

						<button 
							type="object" 
							class="oe_highlight"
							name="test_correct" 
							
							string="Correct" 								
						/>

-->











						<!-- Clear --> 
						<!-- Create Sales --> 
						<!-- Create Patients --> 
						<!-- Import TXT --> 
<!--
						<button 
							type="object" 
							class="oe_highlight oe_right"
							name="clear" 							
							string="Clear" 								
						/>

						<button 
							type="object" 
							class="oe_highlight oe_right"
							name="create_sales" 							
							string="Create Sales" 								
						/>

						<button 
							type="object" 
							class="oe_highlight oe_right"	
							name="create_patients" 
							string="Create Patients" 								
						/>


						<button 
							type="object" 
							class="oe_highlight oe_right"
							name="import_txt" 
							
							string="Import Txt" 								
						/>
-->


						
						<!-- Sync Txt --> 
<!--						
						<button 
							type="object" 
							class="oe_highlight"
							name="sync_txt" 
							
							string="Sync Txt" 								
						/>
-->

					</header>




					<!-- Sheet -->	
					<sheet>

						<group>
							<group>								
								<field name="name"/>
							</group>
							<group>								
								<field name="mgt"/>
								<field name="patient"/>
								<field name="doctor"/>
<!--
								<field name="patient" />
								<field name="partner" />
								<field name="treatment" />
								<field name="doctor" />
-->

							</group>
						</group>

						<group>
							<group>								
								<field name="export_date_begin"/>
								<field name="export_date_end"/>
							</group>
							<group>
<!--
								<field name="export_date"/>
-->						
								<field name="ticket_receipt_create"/>
								<field name="ticket_invoice_create"/>
								<field name="cn_receipt_create"/>
								<field name="cn_invoice_create"/>
<!--
								<field name="receipt_create"/>
								<field name="invoice_create"/>
-->
							</group>
						</group>




						<!-- Padding -->
						<group>
							<group></group>
							<group></group>
						</group>


						<!-- Notebook --> 
						<notebook>
							
							<page string="Patients">
								<field name="patient_ids"/>
							</page>


							<page string="Electronic">
								<field name="electronic_order_ids"/>
							</page>


							<page string="TXT">

								<field name="txt_ids"/>
							
							</page>
						

							<page string="MSSOFT">
							
								<field name="txt_ref_ids"/>
							
							</page>



						</notebook>
					</sheet>
				</form>
			</field>
		</record>
		<!-- FORM --> 






<!-- TREE --> 
		<record model="ir.ui.view" id="container_tree_view">
			<field name="name">container.tree</field>
			<field name="model">openhealth.container</field>
			<field name="arch" type="xml">

				<tree string="Container Tree">

					<field name="name"/>

					<field name="patient_ids"/>

				</tree>

			</field>
		</record>






<!-- ACTION -->
		<record model="ir.actions.act_window" id="container_list_action">
			<field name="name">Containers</field>
			<field name="res_model">openhealth.container</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
            <field name="context">{}</field>            

			<field name="domain">[]</field>

			<field name="limit">20</field>

            <field name="view_id" eval="False"/>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">Create the first container
				</p>
			</field>
		</record>


	</data>
</openerp>
