							<!-- Doctores --> 
							<page   string="Por Doctor">
							  <field name="doctor_line">
							    <tree string="Doctor">                                          
							      <!-- Open Button -->
							      <button 
							        type="object" 
							        class="oe_highlight"    
							        name="open_line_current" 
							        string="e"
							      />
