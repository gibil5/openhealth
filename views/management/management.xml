<!-- FORM --> 
		<record model="ir.ui.view" id="management_form_view">
			<field name="name">management.form</field>
			<field name="model">openhealth.management</field>
			<field name="arch" type="xml">
				<form string="Management Form">

<!-- Header -->  
					<header>
						<!-- Update all -->
						<button 
								type="object"
								class="oe_highlight"
								name="update"
								string="UPDATE"
						/>

						<!-- serialize -->
						<button 
								type="object"
								class="oe_highlight"
								name="serialize"
								string="Serialize"
						/>


						<!-- Update fast -->
						<button 
								type="object"
								class="oe_highlight"
								name="update_fast"
								string="Actualizar"
						/>

						<!-- Update patients -->
						<button 
								type="object" 
								class="oe_highlight"
								name="update_patients"								
								string="Analisis Paciente"
						/>

						<!-- Update Doctors -->
						<button 
								type="object" 
								class="oe_highlight"
								name="update_doctors"
								string="Analisis Doctor"
						/>

						<!-- Update Productivity -->
						<button 
								type="object" 
								class="oe_highlight"
								name="update_productivity"
								string="Act Productividad"                             
						/>

						<!-- Update Doctor Daily -->
						<button 
								type="object" 
								class="oe_highlight"
								name="update_daily"								
								string="Act Diario"                             
						/>

						<!-- List Sales -->
						<button 
						    class="oe_highlight"
						    type="action" 
						    context="{
						          'search_default_management_id': active_id,  
						        }"
						    name="%(management_order_line_form_action)d"
						    string="Ventas" 
						/>

						<!-- List Doctors -->
						<button
						    class="oe_highlight"
						    type="action"
						    context="{
						          'search_default_management_id': active_id,
						        }"
						    name="%(management_doctor_line_form_action)d"
						    string=" Doctores"
						/>

						<!-- See Doctor Daily -->
						<button
						    class="oe_highlight"
						    type="action" 
						    context="{
						          'search_default_management_id': active_id,  
						        }"
						    name="%(doctor_daily_list_action)d" 
						    string="Diario"   
						/>

						<!-- See Productivity -->
						<button
						    class="oe_highlight"
						    type="action" 
						    context="{
						          'search_default_management_id': active_id,  
						        }"
						    name="%(productivity_day_list_action)d" 
						    string="Productividad"   
						/>

						<!-- Check Stats -->
