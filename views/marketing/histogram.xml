<?xml version="1.0"?>
<openerp>
	<data>




		<!-- Tree - Histo Line --> 
		<record model="ir.ui.view" id="histo_line_view_order_tree">

			<field name="name">openhealth.histo.line.tree</field>

			<field name="model">openhealth.histo.line</field>
			
			<field name="arch" type="xml">

				<tree string="Histo Line Tree">
					
<!--
					<field name="idx"/>
					<field name="x_bin"/>
-->
					<field name="x_bin_name"/>




					<field name="count_f"/>
					<field name="percentage_f"/>
					<field name="total_f"/>


					<field name="count_m"/>
					<field name="percentage_m"/>
					<field name="total_m"/>


					<field name="count"/>
					<field name="percentage"/>
					<field name="total"/>

				</tree>
			</field>
		</record>







		<!-- Action -->
		<record model="ir.actions.act_window" id="histo_line_form_action">
			<field name="name">Histograma</field>
			<field name="res_model">openhealth.histo.line</field>
			<field name="view_type">form</field>
<!--
			<field name="view_mode">tree,form,graph,pivot</field>
-->
			<field name="view_mode">tree</field>

			<field name="domain">[
									('marketing_id', '=', active_id),
								]
			</field>

			<field name="limit">1000</field>
		</record>


	</data>
</openerp>
