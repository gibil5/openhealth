<?xml version="1.0"?>
<openerp>
	<data>



		<!-- Form --> 
		<record model="ir.ui.view" id="procedure_cos_form_view">
			<field name="name">procedure.cos.form</field>
			
			<field name="model">openhealth.procedure.cos</field>

			<field name="inherit_id" ref="procedure_form_view"/>
	        <field name="mode">primary</field>
			<field name="arch" type="xml">				



				<!-- <xpath expr="//field[@name='title']" position="before"> -->



				<!-- Title --> 
				<xpath expr="//h1" position="replace">

					<h1>Procedimiento - Cos</h1>					
        		
        		</xpath>



        		<!-- Therapist --> 
				<xpath expr="//field[@name='doctor']" position="replace">
					<field name="therapist"	/>
				</xpath>



        		<!-- Cosmetoloty --> 
				<xpath expr="//field[@name='treatment']" position="replace">
					<field name="cosmetology"	/>
				</xpath>





        		<!-- Therapist --> 
				<xpath expr="//field[@name='appointment']" position="after">
					
					<field name="vspace"/>

					<field name="machine_cos"/>
				</xpath>





			</field>						
		</record>		

	</data>
</openerp>

