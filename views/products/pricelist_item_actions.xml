<?xml version="1.0"?>
<openerp>
	<data>



<!-- Pricelists -->


		<!-- All --> 
		<record model="ir.actions.act_window" id="product_pricelist_item_action">
			<field name="name">Productos</field>
			<field name="res_model">product.pricelist.item</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,kanban,form</field>
            <field name="context">{}</field>
            <field name="domain">[]</field>
		</record>




		<!-- Products --> 
		<record model="ir.actions.act_window" id="product_pricelist_item_product_action">
			<field name="name">Pricelist Productos</field>
			<field name="res_model">product.pricelist.item</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,kanban,form</field>
            <field name="context">{}</field>

            <field name="domain">[
									('x_type', 'in', ['product']),
            	]</field>		
		</record>




		<!-- Co2 --> 
		<record model="ir.actions.act_window" id="product_pricelist_item_consultation_action">
			<field name="name">Pricelist Co2</field>
			<field name="res_model">product.pricelist.item</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,kanban,form</field>
            <field name="context">{}</field>
            
            <field name="domain">[
									('x_type', 'in', ['consultation']),
            	]</field>
		</record>









	</data>
</openerp>
