				<tree string="Product Tree - Mod"    default_order="name" >

					<field name="name"/>

					<field name="description"/>

					<field name="default_code"/>
					
					<field name="id"/>

					<field name="uom_id"	invisible="0"/>



					<field name="price"/>

					<field name="list_price"/>

					<field name="categ_id"/>

					<field name="type"/>

					<field name="state"	invisible="1"	/>				
				</tree>
			</field>
		</record>

