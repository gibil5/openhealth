<?xml version="1.0"?>
<openerp>
	<data>



<!-- FORM -->
		<record model="ir.ui.view" id="product_form">

			<field name="name">product.template.product.form</field>
			
			<field name="model">product.template</field>
			
			<field name="inherit_id" ref="product.product_template_form_view"/>			

	        <field name="mode">primary</field>


	        <field name="priority" eval="1"/>


			<field name="arch" type="xml">




				<xpath expr="//field[@name='invoice_policy']" position="after">

						<field name="default_code"/>

				</xpath>



				<!-- Notebook --> 
				<!--
				<xpath expr="//notebook" position="inside">
				<xpath expr="//page" position="before">
					<page string="Clínica Chavarri"  			sequence="1">
				-->
				<xpath expr="//notebook" position="inside">

					<page string="Clínica Chavarri">


						<group>

							<field name="description"/>

							<field name="x_name_short"/>							
							<field name="x_price_vip"/>
							<field name="x_price_vip_return"/>


<!--
							<field name="x_date_created"/>
							<field name="x_date_updated"/>
-->						
							<field name="x_origin"/>							
							<field name="x_family"/>
							<field name="x_treatment"/>				
							<field name="x_zone"/>				
							<field name="x_pathology"/>				
							<field name="x_level"/>				
							<field name="x_sessions"/>	
							<field name="x_time"/>				

						</group>



						<group>

								<button 
										type="object" 
										class="oe_highlight"

										name="update_level"

										string="Update"
								/>

						</group>

					
					</page>
				</xpath>
			</field>
		</record>

<!--
							<field name="x_client_type"/>				
							<field name="type"/>
							<field name="tax"/>
							<field name="list_price"/>
-->
















