<?xml version="1.0"?>
<openerp>
    <data>


<!-- TREE -->
        <record model="ir.ui.view" id="product_min_tree_view">
            <field name="name">openhealth.product.min.tree</field>

            <field name="model">openhealth.product.min</field>
            
            <field name="type">tree</field>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <tree string="Product Tree - Min">

                    <field name="x_type"/>

                    <field name="new"/>

                    <field name="code"/>
                    <field name="name"/>

                    <field name="family"/>
                    <field name="subfamily"/>


                    <field name="treatment"/>
                    <field name="zone"/>
                    <field name="pathology"/>
                    <field name="level"/>
                    <field name="sessions"/>

                    <field name="time"/>


                    <field name="manufacturer"/>
                    <field name="brand"/>

                    <field name="price"/>
                    <field name="price_vip"/>
                    <field name="price_company"/>
<!--
-->
                </tree>
            </field>
        </record>



<!-- ACTION --> 
        <record model="ir.actions.act_window" id="product_min_list_action">
            <field name="name">Productos Min</field>

            <field name="res_model">openhealth.product.min</field>
            
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form</field>
            <field name="context">{}</field>
            <field name="domain">[]</field>
        </record>


    </data>
</openerp>
