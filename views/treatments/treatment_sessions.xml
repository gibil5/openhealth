<?xml version="1.0"?>
<openerp>
	<data>


		<!--
		sessions
		-->
		<record model="ir.ui.view" id="treatment_session_form_view">
			<field name="name">treatment.session.form</field>
			<field name="model">openhealth.treatment</field>


			<field name="priority" eval="6"/>

			
			<field name="inherit_id" ref="treatment_form_view"/>
			<field name="arch" type="xml">
				<notebook position="inside">
					
					

					<page string="Sesiones">


<!--
						<field name="session_ids"/>
-->



						<field name="session_ids">
							<tree string="Sesiones"		create="false" >

								<button 
									type="object" 
									class="oe_highlight"	
									string="Entrar"
									
									name="open_line_current" 
								/>



								<field name="name"/>
								<field name="appointment"/>
								<field  name="patient"/>
								<field  name="doctor"/>
								<field name="chief_complaint"/>
								<field name="product"/>
								<field name="evaluation_start_date"/>
							</tree>
						</field>

					</page>



				</notebook>
			</field>
		</record>
	</data>
</openerp>
